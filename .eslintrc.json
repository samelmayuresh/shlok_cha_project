{
    "extends": [
        "next/core-web-vitals",
        "next/typescript"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_"
            }
        ],
        "@typescript-eslint/no-explicit-any": "warn",
        "react/jsx-no-target-blank": "error",
        "react-hooks/exhaustive-deps": "warn",
        "no-console": [
            "warn",
            {
                "allow": [
                    "error",
                    "warn"
                ]
            }
        ],
        "prefer-const": "error",
        "eqeqeq": [
            "error",
            "always"
        ],
        "no-var": "error"
    }
}